{
  "name": "idream-admin-portal",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^1.13.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.11.0",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@craco/craco": "5.9.0",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "cross-env": "^10.1.0",
    "env-cmd": "^10.1.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development PORT=3001 craco start",
    "start:dev": "cross-env NODE_ENV=development PORT=3001 env-cmd -f .env.dev craco start",
    "start:prod": "cross-env NODE_ENV=production PORT=3001 env-cmd -f .env.prod craco start",
    "build": "cross-env NODE_ENV=production PORT=3001 craco build",
    "build:dev": "cross-env NODE_ENV=development env-cmd -f .env.dev craco build",
    "build:prod": "cross-env NODE_ENV=production env-cmd -f .env.prod craco build",
    "test": "craco test",
    "test:watch": "craco test --watch",
    "test:coverage": "craco test --coverage",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "proxy": "http://localhost:5000",
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    }
  },
  "overrides": {
    "fork-ts-checker-webpack-plugin": {
      "schema-utils": {
        "ajv": "^6.12.6",
        "ajv-keywords": "^3.5.2"
      }
    },
    "babel-loader": {
      "schema-utils": {
        "ajv": "^6.12.6",
        "ajv-keywords": "^3.5.2"
      }
    },
    "schema-utils": {
      "ajv": "^8.17.1"
    },
    "ajv-formats": {
      "ajv": "^8.17.1"
    }
  }
}
